    "version": "0.2.0",
    "configurations": [
        {
            "type": "gdb",
            "gdbpath": "arm-none-eabi-gdb",
            "request": "attach",
            "name": "attach to RPI2b",
            "executable": "${workspaceFolder}/myos.elf",
            "target": ":1234",
            "remote": true,
            "cwd": "${workspaceRoot}",
            "valuesFormatting": "parseText",
        },
        {
            "type": "gdb",
            "gdbpath": "aarch64-elf-gdb",
            "request": "attach",
            "name": "attach to RPI4",
            "executable": "${workspaceFolder}/myos.elf",
            "target": ":1234",
            "remote": true,
            "cwd": "${workspaceRoot}",
            "valuesFormatting": "parseText",
        },
    ],
    
}